#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chanwit_y/Desktop/Projects/banpu/vuepress-playground/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.9.0__vue@3.5.12/node_modules/vuepress/bin/node_modules:/Users/chanwit_y/Desktop/Projects/banpu/vuepress-playground/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.9.0__vue@3.5.12/node_modules/vuepress/node_modules:/Users/chanwit_y/Desktop/Projects/banpu/vuepress-playground/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.9.0__vue@3.5.12/node_modules:/Users/chanwit_y/Desktop/Projects/banpu/vuepress-playground/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chanwit_y/Desktop/Projects/banpu/vuepress-playground/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.9.0__vue@3.5.12/node_modules/vuepress/bin/node_modules:/Users/chanwit_y/Desktop/Projects/banpu/vuepress-playground/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.9.0__vue@3.5.12/node_modules/vuepress/node_modules:/Users/chanwit_y/Desktop/Projects/banpu/vuepress-playground/node_modules/.pnpm/vuepress@2.0.0-rc.18_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.9.0__vue@3.5.12/node_modules:/Users/chanwit_y/Desktop/Projects/banpu/vuepress-playground/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress-vite.js" "$@"
fi
